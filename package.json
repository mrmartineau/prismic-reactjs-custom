{
  "name": "prismic-reactjs-custom",
  "description": "This is an opinionated fork of prismic-reactjs package  that allows you to use custom React components instead of standard HTML tags",
  "version": "1.0.2",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "repository": {
    "url": "git@github.com:mrmartineau/prismic-reactjs-custom.git",
    "type": "git"
  },
  "author": {
    "url": "https://zander.wtf",
    "name": "Zander Martineau"
  },
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "watch": "rollup -c -w",
    "test": "jest",
    "lint": "tslint ./src/**/*.{ts,tsx}",
    "prepack": "npm run build",
    "presize": "npm run build",
    "size": "size-limit",
    "presize:why": "npm run build",
    "size:why": "size-limit --why"
  },
  "dependencies": {
    "prismic-helpers": "^1.0.3",
    "prismic-richtext": "^1.0.3"
  },
  "devDependencies": {
    "@testing-library/react": "^11.0.4",
    "@types/jest": "^26.0.14",
    "@types/react": "^16.9.49",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rollup": "^2.28.2",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-jsx": "^1.0.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.3",
    "size-limit": "^4.6.0",
    "ts-jest": "^26.4.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.0.3"
  },
  "peerDependencies": {
    "react": "^16.0.0 || ^17.0.1",
    "react-dom": "^16.0.0 || ^17.0.1"
  },
  "size-limit": [
    {
      "path": "dist/es/index.js",
      "limit": "5 KB"
    }
  ]
}
