{
  "name": "prismic-reactjs-custom",
  "description": "This is an opinionated fork of prismic-reactjs package  that allows you to use custom React components instead of standard HTML tags",
  "version": "0.3.1",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "repository": {
    "url": "git@github.com:mrmartineau/prismic-reactjs-custom.git",
    "type": "git"
  },
  "author": {
    "url": "https://zander.wtf",
    "name": "Zander Martineau"
  },
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "watch": "rollup -c -w",
    "test": "jest",
    "lint": "tslint ./src/**/*.{ts,tsx}",
    "prepack": "npm run build",
    "presize": "npm run build",
    "size": "size-limit",
    "presize:why": "npm run build",
    "size:why": "size-limit --why"
  },
  "dependencies": {
    "prismic-helpers": "^1.0.0",
    "prismic-richtext": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/react": "^16.8.1",
    "jest": "^24.1.0",
    "prettier": "^1.16.4",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-testing-library": "^5.4.4",
    "rollup": "^1.1.2",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-jsx": "^1.0.3",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript2": "^0.19.2",
    "size-limit": "^0.21.1",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.3.1"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "20 KB"
    }
  ]
}
